{
  "name": "api-github",
  "version": "1.0.0",
  "description": "Simple GitHub API for pull request portfolio",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:manual": "cross-env PORT=3015 ts-node src/index.ts",
    "dev:e2e": "cross-env PORT=3015 ts-node src/index.ts",
    "dev:vercel": "vercel dev",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy": "vercel --prod",
    "test": "echo \"No tests yet\" && exit 0",
    "generate-static-data": "ts-node src/scripts/generateStaticData.ts"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vercel": "^33.0.1"
  },
  "engines": {
    "node": ">=18.x"
  }
}
