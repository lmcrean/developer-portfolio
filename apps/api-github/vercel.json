{
  "version": 2,
  "name": "api-github",
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/github/(.*)",
      "dest": "/src/index.ts"
    },
    {
      "src": "/health",
      "dest": "/src/index.ts"
    }
  ],
  "env": {
    "GITHUB_TOKEN": "@github_token",
    "GITHUB_USERNAME": "lmcrean"
  }
} 